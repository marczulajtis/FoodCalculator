@model List<FoodCalculator.Models.Category>

<h3>Categories list</h3>

<table class="table table-inverse margin-top">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var category in Model)
        {
            <tr>
                <td>@category.CategoryID</td>
                <td>@category.CategoryName</td>
            </tr>
        }
    </tbody>
</table>



